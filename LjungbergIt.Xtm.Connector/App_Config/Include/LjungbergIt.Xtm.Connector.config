<?xml version="1.0" encoding="utf-8" ?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/">
  <sitecore>
    <processors>
      <XtmConnectorPipeline>
        <processor type="LjungbergIt.Xtm.Connector.Pipelines.XtmProcessor, LjungbergIt.Xtm.Connector" method="AddItemForTranslation" />
      </XtmConnectorPipeline>
      <XtmConnectorPipelineTranslationProgress>
        <processor type="LjungbergIt.Xtm.Connector.Pipelines.XtmProcessor, LjungbergIt.Xtm.Connector" method="ViewTranslationProgress" />
      </XtmConnectorPipelineTranslationProgress>
      <XtmConnectorPipelineTranslationDetailedView>
        <processor type="LjungbergIt.Xtm.Connector.Pipelines.XtmProcessor, LjungbergIt.Xtm.Connector" method="TranslationDetailedView" />
      </XtmConnectorPipelineTranslationDetailedView>
      <XtmConnectorPipelineViewDocumentation>
        <processor type="LjungbergIt.Xtm.Connector.Pipelines.XtmProcessor, LjungbergIt.Xtm.Connector" method="ViewDocumentation" />
      </XtmConnectorPipelineViewDocumentation>
      <XtmConnectorPipelineTranslationReport>
        <processor type="LjungbergIt.Xtm.Connector.Pipelines.XtmProcessor, LjungbergIt.Xtm.Connector" method="TranslationReport" />
      </XtmConnectorPipelineTranslationReport>
      <XtmConnectorPipelineExcludedFields>
        <processor type="LjungbergIt.Xtm.Connector.Pipelines.XtmProcessor, LjungbergIt.Xtm.Connector" method="ExcludedFields" />
      </XtmConnectorPipelineExcludedFields>
    </processors>
    <commands>
      <command name="ljungbergit:AddItemForTranslation" type="LjungbergIt.Xtm.Connector.Buttons.AddItemForTranslation, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:ViewTranslationProgress" type="LjungbergIt.Xtm.Connector.Buttons.ViewTranslationProgress, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:TranslationDetailedView" type="LjungbergIt.Xtm.Connector.Buttons.TranslationDetailedView, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:StartTranslation" type="LjungbergIt.Xtm.Connector.Buttons.StartTranslation, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:GetXtmTemplates" type="LjungbergIt.Xtm.Connector.Buttons.GetXtmTemplates, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:ViewDocumentation" type="LjungbergIt.Xtm.Connector.Buttons.ViewDocumentation, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:TranslationReport" type="LjungbergIt.Xtm.Connector.Buttons.TranslationReport, LjungbergIt.Xtm.Connector"/>
      <command name="ljungbergit:ExcludedFields" type="LjungbergIt.Xtm.Connector.Buttons.ExcludedFields, LjungbergIt.Xtm.Connector"/>
      <!--<command name="test:TestSecurity" type="Test.Buttons.TestSecurity, LjungbergIt.Xtm.Connector"/>-->
    </commands>
  </sitecore>
</configuration>